{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "appSharedResourceGroup": {
            "value": "__appSharedResourceGroup__"
        },
        "appSharedStorageAccountName": {
            "value": "__appSharedStorageAccountName__"
        },
        "aseHostingEnvironmentName": {
            "value": "__aseHostingEnvironmentName__"
        },
        "aseResourceGroup": {
            "value": "__aseResourceGroup__"
        },
        "CosmosDbResourceGroup": {
            "value": "__appSharedResourceGroup__"
        },
        "cosmosDBName": {
            "value": "__appSharedCosmosDBName__"
        },
        "cosmosDbKey": {
            "value": "__appSharedCosmosPrimaryKey__"
        },
        "environment": {
            "value": "__ReleaseEnvironmentName__",
            "metadata": {
                "comment": "Bug in tokenisation task prevents referencing Release.EnvironmentName directly.  https://github.com/TotalALM/VSTS-Tasks/issues/29"
            }
        },
        "sharedAppServicePlanName": {
            "value": "__sharedAppServicePlanName__"
        },
        "sharedAppServicePlanResourceGroup": {
            "value": "__appSharedResourceGroup__"
        },
        "cosmosDbCollectionName": {
            "value": "__cosmosDbCollectionName__"
        },
        "contactusAppDatabaseNeedsCreation": {
            "value": __CosmosDbDatabaseDoesntExist__
        },
        "contactusAppDatabaseThroughput": {
            "value": __contactusAppDatabaseThroughput__
        },
        "chatOptionsChatUrl": {
            "value": "__chatOptionsChatUrl__"
        },
        "enableAlerts": {
            "value": __EnableAzureMonitorAlerting__
        },
        "chatOptionsHideProductionWarning": {
            "value": __chatOptionsHideProductionWarning__,
            "metadata": {
                "comment": "False for all environment except prodcution"
            }
        }
    }
}